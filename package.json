{
    "name": "vscode-anole",
    "displayName": "Anole-Lang",
    "publisher": "Mu001999",
    "description": "For Anole Programming Language",
    "version": "0.0.10",
    "engines": {
        "vscode": "^1.42.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
